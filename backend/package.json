{
  "name": "avy-tracker-backend",
  "version": "1.0.0",
  "description": "Backend for Avy Tracker",
  "main": "server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node server.ts",
    "build": "tsc",
    "serve": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "migrate": "node-pg-migrate",
    "validate-config": "npx ts-node src/scripts/validateConfig.ts",
    "setup-db": "createdb shift_tracker_db && psql -d shift_tracker_db -c 'CREATE EXTENSION IF NOT EXISTS postgis;'",
    "reset-db": "dropdb --if-exists shift_tracker_db && npm run setup-db && npm run migrate",
    "setup-face-verification": "npx ts-node src/scripts/setupFaceVerification.ts",
    "test-face-verification": "npx ts-node src/scripts/testFaceVerification.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@socket.io/postgres-adapter": "^0.5.0",
    "@types/aws-sdk": "^2.7.4",
    "@types/cron": "^2.4.3",
    "@types/csv-parse": "^1.2.5",
    "@types/ioredis": "^5.0.0",
    "@types/multer": "^2.0.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "cron": "^4.3.5",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "expo-server-sdk": "^4.0.0",
    "express": "^5.1.0",
    "firebase-admin": "^13.6.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-schedule": "^2.1.1",
    "nodemailer": "^7.0.11",
    "pg": "^8.16.3",
    "react-native-vision-camera": "^4.7.3",
    "socket.io": "^4.8.1",
    "twilio": "^5.10.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/date-fns": "^2.6.3",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/node-schedule": "^2.1.8",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}

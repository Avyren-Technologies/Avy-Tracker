    "build:android:staging": "eas build --profile staging --platform android --clear-cache --no-wait",
    "build:web:production": "eas build --profile production --platform web --clear-cache --no-wait",
    "build:web:staging": "eas build --profile staging --platform web --clear-cache --no-wait",
    "type-check": "tsc --noEmit --project .",
    "installed-deps": "node ./scripts/find-used-deps.js"
